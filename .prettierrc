{
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "none",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"fluid": false,
	"sortingMethod": "lineLength",
	"stripNewlines": true,
	"plugins": ["prettier-plugin-classify-imports"],
	"importOrder": ["^[./]|(~/)|(@/)"]
}
